{
	"settings": {
		"number_of_replicas": 1,
		"number_of_shards": 5,
		"analysis": {
			"analyzer": {
				"email": {
					"tokenizer": "letter",
					"filter": ["lowercase", "stop"]
				}
			}
		},
        "index": {
            "mapper": {
                "dynamic": false
            }
        }
	},
	"mappings": {
		"jobs_default": {
			"_all" : {
				"enabled" : true,
				"analyzer": "snowball"
			},
			"date_detection": false,
			"numeric_detection": false,
			"dynamic": "strict",
            "properties" : {
                "command" : {
                    "type" : "string",
                    "index": "not_analyzed"
                },
                "component" : {
                    "type" : "string",
                    "index": "not_analyzed"
                },
                "createdTime" : {
                    "type" : "date",
                    "format" : "dateOptionalTime"
                },
                "durationSeconds" : {
                    "type" : "long"
                },
                "endTime" : {
                    "type" : "date",
                    "format" : "dateOptionalTime"
                },
                "id" : {
                    "type" : "long"
                },
                "lockName" : {
                    "type" : "string",
                    "index": "not_analyzed"
                },
                "params" : {
                    "properties" : {
                        "account" : {
                            "type" : "string"
                        },
                        "config" : {
                            "type" : "string"
                        }
                    }
                },
                "process" : {
                    "properties" : {
                        "host" : {
                            "type" : "string",
                            "index": "analyzed",
                            "fields": {
                                "raw": {
                                    "type": "string",
                                    "index": "not_analyzed"
                                }
                            }
                        },
                        "pid" : {
                            "type" : "long"
                        }
                    }
                },
                "project" : {
                    "properties" : {
                        "id" : {
                            "type" : "long"
                        },
                        "name" : {
                            "type" : "string",
                            "index": "analyzed",
                            "fileds": {
                                "raw": {
                                    "type": "string",
                                    "index": "not_analyzed"
                                }
                            }
                        }
                    }
                },
                "result" : {
                    "properties" : {
                        "message" : {
                            "type" : "string"
                        },
                        "status" : {
                            "type" : "string"
                        }
                    }
                },
                "runId" : {
                    "type" : "string",
                    "index": "not_analyzed"
                },
                "startTime" : {
                    "type" : "date",
                    "format" : "dateOptionalTime"
                },
                "status" : {
                    "type" : "string",
                    "index": "not_analyzed"
                },
                "token" : {
                    "properties" : {
                        "description" : {
                            "type" : "string",
                            "fields": {
                                "raw": {
                                    "type": "string",
                                    "index": "not_analyzed"
                                }
                            }
                        },
                        "id" : {
                            "type" : "long"
                        },
                        "token" : {
                            "type" : "string",
                            "index": "not_analyzed"
                        }
                    }
                }
            }
		}
	}
}
